<?xml version='1.0' encoding='utf-8'?>
<component
    name='ZBrightCovePlayer'
    extends='BaseVideoPlayer'
    vm='BrightCovePlayerVM'>

  <interface>
    <field
        id='videoPlayerHeight'
        value='1080'
        type='integer' />
    <field
        id='videoPlayerWidth'
        value='1920'
        type='integer' />
    <field
        id='videoNodeSeek'
        alwaysnotify='true'
        type='integer' />
    <field
        id='videoNodeSeekMode'
        alwaysnotify='true'
        type='string' />
    <field
        id='nextVideoPlay'
        alwaysnotify='false'
        type='boolean' />
    <function name='setInlineView' />

  </interface>
  <children>
  </children>

</component>